{
    "name": "hofmannsven/laravel-modernizr-command",
    "description": "Laravel artisan console command to generate custom Modernizr builds.",
    "keywords": [
        "artisan",
        "console command",
        "laravel",
        "modernizr"
    ],
    "homepage": "https://github.com/hofmannsven/laravel-modernizr-command",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Sven Hofmann",
            "email": "info@hofmannsven.com",
            "homepage": "https://hofmannsven.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@hofmannsven.com",
        "issues": "https://github.com/hofmannsven/laravel-modernizr-command/issues",
        "wiki": "https://github.com/hofmannsven/laravel-modernizr-command/wiki",
        "source": "https://github.com/hofmannsven/laravel-modernizr-command"
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0",
        "symfony/process": "^4.2|^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Hofmannsven\\Modernizr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hofmannsven\\Modernizr\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hofmannsven\\Modernizr\\ModernizrServiceProvider"
            ]
        }
    }
}
